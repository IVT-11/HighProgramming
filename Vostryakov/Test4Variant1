#include <stdio.h>
#define SIZE 10

void strToInt(char [], int);

int main(int argc, char const *argv[])
{
   char arr[] = {'a', '9', 'w', '5', '7', '8', 'e', 'd', '1', 'h'};
   for (int i = 0; i < SIZE; i++)
      printf("%c ", arr[i]);

   printf("\n");

   strToInt(arr, SIZE);
   for (int i = 0; i < SIZE; i++)
      printf("%c ", arr[i]);

   return 0;
}

void strToInt(char arr[], int size)
{
   for (int i = 0; i < size; i++)
      switch (arr[i])
      {
      case 0x30:
         arr[i] = '#'; break;
      case 0x31:
         arr[i] = '#'; break;
      case 0x32:
         arr[i] = '#'; break;
      case 0x33:
         arr[i] = '#'; break;
      case 0x34:
         arr[i] = '#'; break;
      case 0x35:
         arr[i] = '#'; break;
      case 0x36:
         arr[i] = '#'; break;
      case 0x37:
         arr[i] = '#'; break;
      case 0x38:
         arr[i] = '#'; break;
      case 0x39:
         arr[i] = '#'; break;
      }
}
