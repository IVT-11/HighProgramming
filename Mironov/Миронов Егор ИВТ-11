#include <stdio.h>
#include <math.h>
#include <stdlib.h>
int main()
{
float a,b,k1,k2,k3;
int m1,m2,m3;
system ("clear");
printf ("m1="); scanf ("%d=",&m1);
printf ("m2="); scanf ("%d=",&m2);
printf ("m3="); scanf ("%d=",&m3);
printf ("a="); scanf ("%f",&a);
printf ("b="); scanf ("%f",&b);  //используй отступы ПОЖАЛУЙСТА
if (m3 !=0 || m2*m2 !=0 || b !=0)
k1=(m1*m2)*m3+m1/(m2*m3)+a/b;
printf ("k1=%f\n",k1);
k2=m1*m2+m2*m3+m3*m1+21;
printf ("k2=%f\n",k2);
if (m1 !=0 || m2 !=0 || m3 !=0);
k3=49%50+3%50*m1+11%50*m2+21%50*m3;
printf ("k3=%f\n",k3);
return 0;
}


