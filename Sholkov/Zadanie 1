#include <stdlib.h>
#include <stdio.h>
#include <math.h>
#include <locale.h>
#include <time.h>
#include <ctype.h>

#define SIZE 4

void func(char str[SIZE])
{
bool is2 = true;
bool is8 = true;
bool is10 = true;
bool is16 = true;

for (int i = 0; i < SIZE; i++)
{
if (str[i] != '0' && str[i] != '1')
{
is2 = false;
break;
}
}
for (int i = 0; i < SIZE; i++)
{
if (str[i] == '8' || str[i] == '9' || !isdigit(str[i]))
{
is8 = false;
break;
}
}
for (int i = 0; i < SIZE; i++)
{
if (!isdigit(str[i]))
{
is10 = false;
break;
}
}
for (int i = 0; i < SIZE; i++)
{
if ((str[i] != 'A' && str[i] != 'B' && str[i] != 'C' && str[i] != 'D' && str[i] != 'E' && str[i] != 'F') || isdigit(str[i]))
{
is16 = false;
break;
}
}
printf("Число может быть:\n");
if (is2)
{
printf("Двоичным\n");
}
if (is8)
{
printf("Восьмиричным\n");
}
if (is10)
{
printf("Десятичным\n");
}
if (is16)
{
printf("Шестнадцатеричным\n");
}
}

int main()
{
setlocale(0, "");
char str[SIZE];
for (int i = 0; i < 4; i++)
{
fgets(str, 100, stdin);
puts(str);
func(str);
}
}
